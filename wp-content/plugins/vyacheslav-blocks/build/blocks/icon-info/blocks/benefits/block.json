{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"textdomain": "pool4u",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"version": "0.1.0",

	"name": "pool4u/benefits",
	"title": "Преимущества компании",
	"category": "design",
	"description": "Отображает фон, текст а также цифры для информации о преимуществах компании",
	"keywords": ["фото и текст", "фото", "текст", "text", "image"],

	"supports": {
		"align": [
			"wide",
			"full"
		]
	},
	"attributes": {
		"align": {
			"type": "string",
			"default": "wide"
		},
		"heading": {
			"type": "string"
		},
		"subheading": {
			"type": "string"
		},
		"preamble1": {
			"type": "string"
		},
		"preamble2": {
			"type": "string"
		},
		"preamble3": {
			"type": "string"
		},
		"imgURL": {
			"type": "string"
		},
		"imgID": {
			"type": "number"
		},
		"imgAlt": {
			"type": "string"
		},
		"imgPlacement": {
			"type": "string",
			"default": "left"
		},
		"imgPosition": {
			"type": "object",
			"default": {
				"x": 0.5,
				"y": 0.5
			}
		}




	}
}
